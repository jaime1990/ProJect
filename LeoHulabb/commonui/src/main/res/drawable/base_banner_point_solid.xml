    </item>
    <item>
        <shape android:shape="oval">
            <size android:width="@dimen/circle_indicator_size"
                  android:height="@dimen/circle_indicator_size" />
            <solid android:color="@color/normal_circle_indicator" />
        </shape>
    </item>
</selector>